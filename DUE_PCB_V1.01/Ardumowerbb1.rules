
(rules PCB Ardumowerbb1
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 4656)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.8)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-Power" "Via[0-1]_800:400_um" Power
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    Power "Via[0-1]_800:400_um-Power"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    SCL1 SDA1 GND +9V pinMotorRightSense pinMotorRightFault IOREV pinMotorRightPWM
    pinMotorEnable pinMotorRightDir pinMotorLeftSense pinMotorLeftFault pinMotorLeftPWM pinMotorLeftDir pinMotorMowSense pinMotorMowFault
    pinMotorMowPWM pinMotorMowDir pinMotorMowEnable pinOdometryRight pinOdometryLeft +24V pinDropLeft pinBumperLeft
    pinBumperRight pinDropRight pinBuzzer pinRemoteSwitch pinButton pinChargeRelay pinUserSwitch3 pinUserSwitch2
    pinUserSwitch1 pinRain pinSonarLeftEcho pinTilt pinSonarLeftTrigger pinSonarRightEcho pinSonarRightTrigger pinSonarCenterTrigger
    pinSonarCenterEcho pinChargeVoltage pinChargeCurrent pinVoltageMeasurement pinPerimeterLeft pinPerimeterRight pinBatteryVoltage pinRemoteSteer
    pinRemoteMow pinLED pinRemoteSpeed pinRedLED pinGreenLED pinBatterySwitch SCL0 SDA0
    "Net-(C1-Pad2)" TX2 RX2 TX1 RX1 pinMotorMowRpm "Net-(D6-Pad2)" "Net-(D1-Pad2)"
    "Net-(D2-Pad2)" "Net-(J12-Pad3)" "Net-(J13-Pad3)" "Net-(J10-Pad3)" "Net-(J11-Pad3)" "Net-(D3-Pad2)" "Net-(D4-Pad2)" "Net-(D5-Pad2)"
    MotorBreak "Net-(J28-Pad6)" "Net-(J28-Pad1)" "Net-(D2-Pad1)" "Net-(D7-Pad2)" "Net-(D8-Pad2)" +3.3VA +5VA
    "Net-(J14-Pad3)" "Net-(J29-Pad6)" "Net-(J29-Pad1)" "Net-(J30-Pad6)" "Net-(J30-Pad1)" "Net-(JP4-Pad2)" "Net-(J28-Pad4)" "Net-(J29-Pad4)"
    "Net-(J30-Pad4)" "Net-(D1-Pad1)" "Net-(D3-Pad1)" "Net-(J4-Pad2)" "Net-(J4-Pad1)" MotorMowBreak +5V +3V3
    "Net-(U1-Pad24)" "Net-(U1-Pad23)" "Net-(U1-Pad20)" "Net-(U1-Pad19)" "Net-(U1-Pad12)" "Net-(U1-Pad11)" "Net-(U1-Pad1)" "Net-(U2-Pad24)"
    "Net-(U2-Pad23)" "Net-(U2-Pad20)" "Net-(U2-Pad19)" "Net-(U2-Pad12)" "Net-(U2-Pad11)" "Net-(U2-Pad1)" "Net-(XA1-PadMISO)" "Net-(XA1-PadD49)"
    pinLawnBackSend pinLawnBackRecv pinLawnFrontSend pinLawnFrontRecv "Net-(XA1-Pad5V4)" "Net-(XA1-Pad5V3)" pinOdometryRight2 pinOdometryLeft2
    "Net-(XA1-PadA10)" pinPerimeterCenter "Net-(XA1-PadD9)" "Net-(XA1-PadD8)" TX0 RX0 "GPS_RX" "GPS_TX"
    Reset "Net-(XA1-Pad5V2)" "Net-(XA1-PadSCK)" "Net-(XA1-PadMOSI)" "Net-(XA1-PadGND4)" "Net-(XA1-PadRST2)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Power
    (clearance_class Power)
    (via_rule Power)
    (rule
      (width 1000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)